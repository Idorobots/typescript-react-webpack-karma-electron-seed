{
  "name": "typescript-react-webpack-karma-electron-seed",
  "version": "0.1.0",
  "description": "A seed project for full-featured TypeScript 2.0 + React + Webpack + Karma + Electron stack",
  "author": "Kajetan Rzepecki <kajtek@idorobots.org>",
  "license": "MIT",
  "main": "dist/app.js",
  "types": "dist/src/main.d.ts",
  "scripts": {
    "lint": "npm run lint-css && npm run lint-ts",
    "lint-css": "stylelint --config './stylelint.json' './src/**/*.css'",
    "lint-ts": "tslint 'src/**/*.tsx' 'src/**/*.ts'",
    "test": "karma start --singleRun true",
    "test:ci": "karma start --singleRun true --browsers 'PhantomJS'",
    "test:watch": "karma start --singleRun false",
    "build": "webpack --bail",
    "start": "electron dist/app.js",
    "package-app": "electron-packager . app",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "posttest": "istanbul check-coverage --statement 80 --branch 50 --function 70"
  },
  "dependencies": {
    "es6-shim": "^0.35.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.5.2"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.35",
    "@types/react": "^0.14.41",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.17",
    "@types/react-router": "^2.0.37",
    "autoprefixer": "^6.3.3",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "electron": "^1.4.3",
    "electron-packager": "^8.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-electron": "^5.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "object-assign": "^4.1.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^6.8.0",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.9.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.5.0",
    "typedoc-webpack-plugin": "^1.0.10",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.6",
    "watchify": "^3.7.0",
    "webpack": "^1.13.0",
    "webpack-split-by-path": "0.0.10"
  }
}
